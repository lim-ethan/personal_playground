   "source": [
    "def solution(destination):\n",
    "    energy = 0\n",
    "    while destination > 0 : \n",
    "        if destination % 2 == 0 : \n",
    "            destination = destination // 2\n",
    "        else : \n",
    "            destination -= 1\n",
    "            energy += 1\n",
    "    return energy"
   ]
