    "def solution(A,B):\n",
    "    answer = 0\n",
    "    A = sorted(A, reverse=True)\n",
    "    B = sorted(B, reverse=False)\n",
    "    for i in range(len(A)) : \n",
    "        answer += A[i] * B[i]\n",
    "    return answer"
   ]
